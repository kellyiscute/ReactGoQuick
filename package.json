{
	"name": "react-ext",
	"displayName": "ReactGoQuick",
	"description": "Create components & contexts with just one key",
	"publisher": "Kelly",
	"version": "1.0.4",
	"icon": "react-logo.png",
	"engines": {
		"vscode": "^1.44.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/guo40020/react-ext"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"keywords": [
		"react",
		"react hook",
		"react snippet"
	],
	"activationEvents": [
		"onCommand:react-ext.CreateComponent",
		"onCommand:react-ext.CreateContext"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [{
				"command": "react-ext.CreateComponent",
				"title": "Create Component"
			},
			{
				"command": "react-ext.CreateContext",
				"title": "Create Context"
			}
		],
		"snippets": [{
				"language": "javascriptreact",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/snipptes.json"
			}
		],
		"configuration": [{
			"title": "ReactGoQuick",
			"properties": {
				"ReactGoQuick.ComponentPath": {
					"type": "string",
					"default": "/src/components",
					"description": "Relative path to all your components"
				},
				"ReactGoQuick.ContextPath": {
					"type": "string",
					"default": "/src/contexts",
					"description": "Relative path to all your contexts"
				}
			}
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
